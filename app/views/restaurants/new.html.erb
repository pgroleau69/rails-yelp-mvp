<h1>New restaurant</h1>


<div class="container pt-5">

  <%= simple_form_for @restaurant do |f| %>
   <%= f.input :name %>
   <%= f.input :address %>
   <%= f.input :category, collection: Restaurant::CATEGORIES %>
   <%= f.input :phone_number %>
   <%= f.submit class: "btn btn-primary" %>
  <% end %>


</div>
